{
    "name": "rolandinsh/php-eregis",
    "description": "Replacement for the ereg legacy php extension.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Republa\\EregShim\\": "src"
        },
        "files": [ "src/bootstrap.php" ]
    },
    "autoload-dev": {
        "psr-4": {
            "Republa\\EregShim\\": "tests"
        }
    },
    "require": {
        "php": "^5.3 || ^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35 || ^5.6 || ^6 || ^7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander M. Turek",
            "email": "me@derrabus.de"
        },
        {
            "name": "Rolands Umbrovskis",
            "email": "info@simplemediacode.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
